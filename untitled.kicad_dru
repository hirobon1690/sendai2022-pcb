(version 1)
(rule "class 0:0"
  (condition "A.NetClass == 'default' && B.NetClass == 'Default'")
  (constraint clearance (min 0.4mm)))

(rule "class 1:1"
  (condition "A.NetClass == '12v' && B.NetClass == '12v'")
  (constraint clearance (min 0.4mm)))

(rule "class 2:2"
  (condition "A.NetClass == '5v' && B.NetClass == '5v'")
  (constraint clearance (min 0.4mm)))
